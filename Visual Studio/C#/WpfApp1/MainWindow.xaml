<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
	<Grid>
		<DataGrid ItemsSource="{Binding Items}" AutoGenerateColumns="False" HorizontalAlignment="Left" Height="340" Margin="60,39,0,0" VerticalAlignment="Top" Width="647">
			<DataGrid.Columns>
				<DataGridTextColumn Header="数据" Binding="{Binding RelativeSource={RelativeSource AncestorType=Control}}"/>
			</DataGrid.Columns>
		</DataGrid>

	</Grid>
</Window>
